---
import Layout from '@layouts/Layout.astro';
import {type StoresType, homeOpened, settingOpened, language} from '@store/store';
homeOpened.set(false)
settingOpened.set(true)
language.set('en')
const Stores:StoresType = {
    isHomeOpened: homeOpened.get(),
    isSettingOpened: settingOpened.get(),
    languageState: language.get()
}
---

<Layout stores={Stores} />