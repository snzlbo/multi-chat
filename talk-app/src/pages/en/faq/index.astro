---
import Layout from '@layouts/Layout.astro';
import {type StoresType, homeOpened, faqOpened, language} from '@store/store';
homeOpened.set(false)
faqOpened.set(true);
language.set('en')
const Stores:StoresType = {
    isHomeOpened: homeOpened.get(),
    isFaqOpened: faqOpened.get(),
    languageState: language.get()
}
---

<Layout stores={Stores} />