---
import Layout from '@layouts/Layout.astro';
import {type StoresType, personaTalkOpened, bottomAppBarOpened, homeOpened} from '@store/store';
personaTalkOpened.set(true);
bottomAppBarOpened.set(false);
homeOpened.set(false);
const Stores:StoresType = {
    isPersonaTalkOpened: personaTalkOpened.get(),
    isBottomAppBarOpened: bottomAppBarOpened.get(),
    isHomeOpened: homeOpened.get()
}
---

<Layout stores={Stores} />